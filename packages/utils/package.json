{
  "name": "@udecode/utils",
  "version": "37.0.0",
  "description": "Udecode utils",
  "license": "MIT",
  "homepage": "https://platejs.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/udecode/plate.git",
    "directory": "packages/utils"
  },
  "bugs": {
    "url": "https://github.com/udecode/plate/issues"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "INIT_CWD=\"$PWD\" pnpm -w run p:build",
    "build:watch": "INIT_CWD=\"$PWD\" pnpm -w run p:build:watch",
    "brl": "INIT_CWD=\"$PWD\" pnpm -w run p:brl",
    "clean": "INIT_CWD=\"$PWD\" pnpm -w run p:clean",
    "lint": "INIT_CWD=\"$PWD\" pnpm -w run p:lint",
    "lint:fix": "INIT_CWD=\"$PWD\" pnpm -w run p:lint:fix",
    "test": "INIT_CWD=\"$PWD\" pnpm -w run p:test",
    "test:watch": "INIT_CWD=\"$PWD\" pnpm -w run p:test:watch",
    "typecheck": "INIT_CWD=\"$PWD\" pnpm -w run p:typecheck"
  },
  "keywords": [
    "utils"
  ],
  "publishConfig": {
    "access": "public"
  }
}
